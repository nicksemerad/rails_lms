<h1> New User </h1>
<%= form_with(model: @user, local: true, class: 'ui form' ) do |f|%>
  <div>
  <%= f.label :first_name %>
  <%= f.text_field :first_name, require: true%>
  </div>

  <div class="ui">
  <%= f.label :last_name%>
  <%= f.text_field :last_name, required: true %>
  </div>

  <%= f.submit nil, class: "ui orange button inverted"%>
<% end %>